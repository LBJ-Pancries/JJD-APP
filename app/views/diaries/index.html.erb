<h1>施工日志</h1>
<div class="col-xs-12 col-sm-9">
  <div class="row">
    <% @diaries.each do |diary| %>
    <h1><%= link_to(diary.title, diary_path(diary))%></h1>
    <%= link_to("Edit", edit_diary_path(diary), class: "btn btn-sm btn-default")%>
    <%= link_to("Delete", diary_path(diary), class: "btn btn-sm btn-default", method: :delete, data: { confirm: "Are you sure?"})%>
    <% end %>
  </div>
</div>
<div class="col-xs-6 col-sm-3">
  <div class="row">
    <%= link_to("New diary", new_diary_path, class: "btn btn-primary pull-right")%>
  </div>
</div>
