<div class="col-md-12">
	<div class="group">
		<%= link_to("New house", new_house_path, class: "btn btn-primary pull-right") %>
	</div>
	<table class="table table-hover">
		<thead>
			<tr>
				<td>ID</td>
				<td>Title</td>
				<td>Description</td>
			</tr>
		</thead>
		<tbody>
			<% @houses.each do |house| %>
				<tr>
					<td><%= house.id %></td>
					<td><%= link_to(house.title, house_path(house))%></td>
					<td><%= house.description %></td>

				</tr>
			<% end %>
		</tbody>
	</table>
</div>
